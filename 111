meme_dict = {
    'РОФЛ': 'шутка'
    'ТФ': 'телефон'
    'ГГ': 'хорошая игра'
    'АПНУТЬ': 'поднять что-либо в игре'
    'ДЕФОЛТ': 'обычный случай'
    }
for i in range(5):
word = input('Напишите какое слово не понятно большими буквами!')
if word in meme_dict.keys():
    #Слово найдено
    print(meme_dict[word])
else:
    #Слово не найдено
    print('Такое слово не найдено')
